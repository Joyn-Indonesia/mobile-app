<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_mservice_progress"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/material_light_white"
    tools:context="com.joyn.me.mService.mServiceProgress">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:adjustViewBounds="true"
            android:background="@color/splash_bg_color"
            android:scaleType="fitXY" />

        <ImageView
            android:id="@+id/btn_home"
            android:layout_width="36dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="12dp"
            android:src="@drawable/ic_arrow_back_yellow"
            android:tint="@color/white"
            android:adjustViewBounds="true"
            android:visibility="visible" />

        <customfonts.TextViewSFProDisplayMedium
            android:id="@+id/mService_title"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="@string/home_mService"
            android:textAllCaps="true"
            android:textColor="@color/material_light_white"
            android:textSize="18sp"
            android:textStyle="bold" />
    </FrameLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/text_technicians"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/material_blue_grey_50">

                <customfonts.TextViewSFProDisplayRegular
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="8dp"
                    android:text="@string/service_progress"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <customfonts.TextViewSFProDisplayMedium
                        android:id="@+id/text_ordernum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:padding="8dp"
                        android:text="ORDER NUMBER"
                        android:textColor="@color/colorPrimary" />

                    <com.makeramen.roundedimageview.RoundedImageView
                        android:id="@+id/driver_image"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:scaleType="centerCrop"
                        app:riv_corner_radius="120dp"
                        app:riv_border_width="2dp"
                        app:riv_border_color="@color/colorPrimary" />

                    <customfonts.TextViewSFProDisplayMedium
                        android:id="@+id/driver_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:textSize="16dp"
                        android:text="Ariana Grande"
                        android:textColor="@color/colorPrimary" />

                    <customfonts.TextViewSFProDisplayRegular
                        android:id="@+id/driver_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="3dp"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:text="0857000000"
                        android:textColor="@color/material_blue_grey_700" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/grey" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_gravity="center"
                        android:background="@color/white">

                        <LinearLayout
                            android:id="@+id/btn_chat"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:layout_weight="1">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:src="@drawable/ic_chat"
                                android:layout_gravity="center"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp" />

                            <customfonts.TextViewSFProDisplayMedium
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:text="Chat"
                                android:textSize="16dp" />
                        </LinearLayout>

                        <View
                            android:layout_width="1dp"
                            android:layout_height="40dp"
                            android:background="@color/grey" />

                        <LinearLayout
                            android:id="@+id/btn_call"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:gravity="center"
                            android:layout_weight="1">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:src="@drawable/ic_phone"
                                android:tint="@color/colorPrimary"
                                android:layout_gravity="center"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp" />

                            <customfonts.TextViewSFProDisplayMedium
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Call"
                                android:textSize="16dp"
                                android:gravity="center_vertical" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal"
                        android:background="@color/material_blue_grey_50">

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/text_service"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/service_layanan"
                            android:textSize="16sp"
                            android:textColor="@color/colorPrimary" />

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/value_service"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/mbox_location"
                            android:textSize="16sp"
                            android:textColor="@color/material_blue_grey_700" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@color/material_blue_grey_200" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal"
                        android:background="@color/material_light_white">

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/text_actype"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/service_actype"
                            android:textSize="16sp"
                            android:textColor="@color/colorPrimary" />

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/value_actype"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/mbox_location"
                            android:textSize="16sp"
                            android:textColor="@color/material_blue_grey_700" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@color/material_blue_grey_200" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal"
                        android:background="@color/material_blue_grey_50">

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/text_quantity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/service_quantity"
                            android:textSize="16sp"
                            android:textColor="@color/colorPrimary" />

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/value_quantity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/mbox_location"
                            android:textSize="16sp"
                            android:textColor="@color/material_blue_grey_700" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@color/material_blue_grey_200" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal"
                        android:background="@color/material_light_white">

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/text_problem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/service_problem"
                            android:textSize="16sp"
                            android:textColor="@color/colorPrimary" />

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/value_problem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/mbox_location"
                            android:textSize="16sp"
                            android:textColor="@color/material_blue_grey_700" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@color/material_blue_grey_200" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal"
                        android:background="@color/material_blue_grey_50">

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/text_location"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/mbox_location"
                            android:textSize="16sp"
                            android:textColor="@color/colorPrimary" />

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/value_location"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/mbox_location"
                            android:textSize="16sp"
                            android:textColor="@color/material_blue_grey_700" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@color/material_blue_grey_200" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal"
                        android:background="@color/material_light_white">

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/text_price"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/service_price"
                            android:textSize="16sp"
                            android:textColor="@color/colorPrimary" />

                        <customfonts.TextViewSFProDisplayRegular
                            android:id="@+id/value_price"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:padding="5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:text="@string/mbox_location"
                            android:textSize="16sp"
                            android:textColor="@color/material_blue_grey_700" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@color/material_blue_grey_200" />

                </LinearLayout>

                <customfonts.Button_SF_Pro_Display_Medium
                    android:id="@+id/btn_cancel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorYellow"
                    android:text="@string/text_cancel"
                    android:textColor="@color/cast_libraries_material_featurehighlight_text_body_color"
                    android:textSize="18sp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>
